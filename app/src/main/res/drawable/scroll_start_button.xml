<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="@color/ripple"
xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item
        android:drawable="@drawable/round_btn_secondary"
        >
    </item>
    <item
        android:width="4dp"
        android:height="20dp"
        android:gravity="center_horizontal|center_vertical"
        >
        <shape
            android:shape="rectangle"
            >
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:width="15dp"
        android:height="4dp"
        android:gravity="center_horizontal|center_vertical"
        android:left="-7.7dp"
        android:top="-12dp"
        >
        <rotate android:fromDegrees="-45">
            <shape
                android:shape="rectangle"
                >
                <solid android:color="#FFFFFF" />
            </shape>
        </rotate>

    </item>
    <item
        android:width="15dp"
        android:height="4dp"
        android:gravity="center_horizontal|center_vertical"
        android:left="7.7dp"
        android:top="-12dp"
        >
        <rotate android:fromDegrees="45">
            <shape
                android:shape="rectangle"
                >
                <solid android:color="#FFFFFF" />
            </shape>
        </rotate>

    </item>
</ripple>
